{
    "name": "yiisoft/yii-bootstrap4",
    "type": "library",
    "description": "Yii Framework Twitter Bootstrap 4 Extension",
    "keywords": [
        "yii",
        "bootstrap4"
    ],
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/yiisoft/yii-bootstrap4",
        "issues": "https://github.com/yiisoft/yii-bootstrap4/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "npm-asset/bootstrap": "^4.5.3",
        "npm-asset/jquery": "^3.5.1",
        "oomphinc/composer-installers-extender": "^2.0.0",
        "yiisoft/arrays": "^3.0@dev",
        "yiisoft/assets": "^1.0@dev",
        "yiisoft/html": "^3.0@dev",
        "yiisoft/json": "^1.0",
        "yiisoft/widget": "^3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "roave/infection-static-analysis-plugin": "^1.5",
        "spatie/phpunit-watcher": "^1.23",
        "vimeo/psalm": "^4.2",
        "yiisoft/aliases": "^1.0",
        "yiisoft/di": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Yii\\Bootstrap4\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Yii\\Bootstrap4\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "installer-types": [
            "npm-asset"
        ],
        "installer-paths": {
            "./node_modules/{$name}": [
                "type:npm-asset"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "scripts": {
        "test": "phpunit --testdox --no-interaction",
        "test-watch": "phpunit-watcher watch"
    }
}
